{
  "harmony": {
    "harmonyName": "default",
    "harmonyHost": "blank",
    "harmonyUsername": "",
    "harmonyPassword": "",
    "bypassLogitechLogin": true
  },
  "mqtt": {
    "brokerIp": "localhost",
    "brokerPort": 1883,
    "brokerUsername": null,
    "brokerPassword": null
  },
  "Serilog": {
    "Enrich": [ "FromLogContext" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "logs/HomeAutio.Mqtt.Harmony.log",
          "retainedFileCountLimit": 31
        }
      }
    ]
  }
}